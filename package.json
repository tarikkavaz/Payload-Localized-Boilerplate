{
  "name": "payload-localized-boilerplate",
  "version": "1.0.0",
  "description": "An example of how to use Payload's localization features",
  "type": "module",
  "scripts": {
    "predev": "pnpm install --frozen-lockfile",
    "prebuild": "pnpm install --frozen-lockfile",
    "prestart": "pnpm install --frozen-lockfile",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:turbo": "cross-env NODE_OPTIONS=--no-deprecation next dev --turbopack",
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation eslint .",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation eslint . --fix",
    "typecheck": "cross-env NODE_OPTIONS=--no-deprecation tsc --noEmit",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm -f pnpm-lock.yaml && pnpm --ignore-workspace install",
    "install:check": "pnpm install --check-files --frozen-lockfile",
    "clean": "rimraf node_modules .next || true"
  },
  "dependencies": {
    "@libsql/client": "^0.15.15",
    "@payloadcms/db-sqlite": "^3.55.1",
    "@payloadcms/email-resend": "^3.55.1",
    "@payloadcms/live-preview-react": "3.55.1",
    "@payloadcms/next": "3.55.1",
    "@payloadcms/payload-cloud": "3.55.1",
    "@payloadcms/plugin-form-builder": "3.55.1",
    "@payloadcms/plugin-nested-docs": "3.55.1",
    "@payloadcms/plugin-redirects": "3.55.1",
    "@payloadcms/plugin-search": "3.55.1",
    "@payloadcms/plugin-seo": "3.55.1",
    "@payloadcms/richtext-lexical": "3.55.1",
    "@payloadcms/ui": "3.55.1",
    "@playwright/test": "^1.55.0",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.2.6",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "framer-motion": "^12.23.15",
    "framer-motion-ticker": "^1.0.19",
    "geist": "^1.3.0",
    "graphql": "^16.8.2",
    "jsonwebtoken": "9.0.2",
    "libsql": "^0.5.22",
    "lucide-react": "^0.378.0",
    "next": "^15.2.3",
    "next-intl": "^3.23.2",
    "payload": "3.55.1",
    "prism-react-renderer": "^2.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "7.45.4",
    "react-icons": "^5.5.0",
    "sharp": "^0.34.3",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@tailwindcss/typography": "^0.5.13",
    "@types/escape-html": "^1.0.2",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "22.5.4",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.1",
    "autoprefixer": "^10.4.19",
    "copyfiles": "^2.4.1",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.1.0",
    "postcss": "^8.4.38",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.3",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  }
}
